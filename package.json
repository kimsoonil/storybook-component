{
  "name": "intro-storybook-react-template",
  "version": "0.1.0",
  "description": "Starter template to get up and running quickly with React and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-react-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-react-template/issues"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@loadable/component": "^5.15.2",
    "@material-ui/core": "4.9.14",
    "@material-ui/pickers": "4.0.0-alpha.7",
    "@mui/material": "^5.10.2",
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/i18next": "^13.0.0",
    "@types/react-i18next": "^8.1.0",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "dayjs": "^1.11.5",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "i18next": "^19.6.3",
    "jodit-react": "^1.3.16",
    "node-sass": "^7.0.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "rc-pagination": "^3.1.17",
    "react": "^18.1.0",
    "react-calendar": "^3.7.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.18.5",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "CI= env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "init-msw": "msw init public/",
    "scan:i18n": "i18next-scanner --config i18next-scanner.config.js",
    "upload:i18n": "npm run scan:i18n && node translate/upload.js",
    "download:i18n": "node translate/download.js",
    "serve": "npm run download:i18n && vue-cli-service serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-essentials": "^6.5.7",
    "@storybook/addon-interactions": "^6.5.7",
    "@storybook/addon-links": "^6.5.7",
    "@storybook/builder-webpack5": "^6.5.7",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.7",
    "@storybook/node-logger": "^6.5.7",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.7",
    "@storybook/testing-library": "^0.0.11",
    "babel-loader": "^8.2.5",
    "babel-plugin-named-exports-order": "^0.0.2",
    "google-spreadsheet": "^3.0.13",
    "html-webpack-plugin": "^5.5.0",
    "i18next-scanner": "^2.11.0",
    "msw": "^0.42.0",
    "msw-storybook-addon": "^1.6.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "eslintIgnore": [
    "ignoredfile.js",
    "ignoredfile2.js"
  ],
  "postinstall": "patch-package"
}
